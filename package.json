{
    "name":  "nexus-vmp-canon",
    "version":  "0.1.0",
    "type":  "module",
    "main":  "server.js",
    "engines":  {
                    "node":  "20.x"
                },
    "scripts":  {
                    "start":  "node server.js",
                    "dev":  "nodemon server.js",
                    "lint":  "eslint . --ext .js,.html",
                    "lint:fix":  "eslint . --ext .js,.html --fix",
                    "format":  "prettier --write \"**/*.{js,json,html,css,md}\"",
                    "format:check":  "prettier --check \"**/*.{js,json,html,css,md}\"",
                    "test":  "vitest run",
                    "test:watch":  "vitest",
                    "test:ui":  "vitest --ui",
                    "test:coverage":  "vitest run --coverage",
                    "test:coverage:report":  "vitest run --coverage --reporter=verbose",
                    "test:browser":  "cross-env VITEST_BROWSER=true vitest run --browser tests/browser/",
                    "test:browser:ui":  "cross-env VITEST_BROWSER=true vitest --browser --ui tests/browser/",
                    "test:browser:watch":  "cross-env VITEST_BROWSER=true vitest --browser tests/browser/",
                    "test:unit":  "vitest run tests/unit/",
                    "test:meta":  "vitest run tests/unit/meta/",
                    "test:integration":  "vitest run tests/integration/",
                    "test:components":  "vitest run tests/unit/components/",
                    "test:adapters":  "vitest run tests/integration/adapters/",
                    "test:utils":  "vitest run tests/unit/utils/",
                    "test:server":  "vitest run tests/integration/server/",
                    "test:soa":  "vitest run tests/integration/server/server-soa-routes.test.js tests/integration/adapters/soa-adapter.test.js",
                    "test:combined":  "npm run test \u0026\u0026 npm run test:e2e",
                    "test:e2e":  "playwright test",
                    "test:e2e:ui":  "playwright test --ui",
                    "test:e2e:headed":  "playwright test --headed",
                    "test:e2e:soa":  "playwright test tests/e2e/workflows/soa-recon-workflow.spec.js",
                    "test:all":  "npm run test:coverage \u0026\u0026 npm run test:e2e",
                    "ci:gate":  "npm run lint && npm run test:unit && node scripts/audit/no-drift-audit.mjs",
                    "vercel-build":  "echo \u0027No build step required for Express\u0027",
                    "set-password":  "node scripts/documented/set-password.js",
                    "setup-jack-password":  "node scripts/documented/setup-jack-password.js",
                    "seed":  "node scripts/documented/seed-vmp-data.js",
                    "vitest:run":  "node scripts/documented/run-vitest.js",
                    "vitest:coverage:run":  "node scripts/documented/run-vitest-coverage.js",
                    "audit:no-drift":  "node scripts/audit/no-drift-audit.mjs",
                    "guardrails":  "node scripts/audit/no-drift-audit.mjs",
                    "validate:docs":  "node scripts/documented/validate-docs-naming.mjs",
                    "validate:scripts":  "node scripts/documented/validate-scripts.mjs",
                    "validate:all":  "npm run audit:no-drift && npm run validate:docs && npm run validate:scripts"
                },
    "dependencies":  {
                         "@supabase/supabase-js":  "^2.39.0",
                         "bcrypt":  "^6.0.0",
                         "chart.js":  "^4.4.0",
                         "compression":  "^1.8.1",
                         "connect-pg-simple":  "^10.0.0",
                         "cookie-parser":  "^1.4.7",
                         "cookie-session":  "^2.0.0",
                         "csv-parse":  "^6.1.0",
                         "dotenv":  "^16.3.1",
                         "envalid":  "^8.1.1",
                         "exceljs":  "^4.4.0",
                         "express":  "^4.18.2",
                         "express-async-errors":  "^3.1.1",
                         "express-rate-limit":  "^8.2.1",
                         "express-session":  "^1.18.2",
                         "express-validator":  "^7.3.1",
                         "helmet":  "^8.1.0",
                         "multer":  "^1.4.5-lts.1",
                         "nodemailer":  "^7.0.12",
                         "nunjucks":  "^3.2.4",
                         "pdf-parse":  "^1.1.4",
                         "pdfkit":  "^0.17.2",
                         "web-push":  "^3.6.7",
                         "winston":  "^3.19.0"
                     },
    "devDependencies":  {
                            "@playwright/test":  "^1.57.0",
                            "@types/cookie-session":  "^2.0.49",
                            "@types/express":  "^5.0.6",
                            "@types/node":  "^25.0.3",
                            "@vitest/browser-playwright":  "^4.0.16",
                            "@vitest/coverage-v8":  "^4.0.16",
                            "@vitest/ui":  "^4.0.16",
                            "cross-env":  "^10.1.0",
                            "eslint":  "^8.57.0",
                            "eslint-config-prettier":  "^10.1.8",
                            "eslint-plugin-html":  "^7.1.0",
                            "eslint-plugin-prettier":  "^5.5.4",
                            "husky":  "^9.1.7",
                            "lint-staged":  "^16.2.7",
                            "nodemon":  "^3.0.2",
                            "playwright":  "^1.57.0",
                            "prettier":  "^3.7.4",
                            "supertest":  "^7.1.4",
                            "vitest":  "^4.0.16"
                        }
}
